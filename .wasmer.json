{
  "name": "cashspace",
  "version": "1.0.0",
  "type": "php",
  "php_version": "8.1",
  "document_root": "/",
  "entry_point": "index.php",
  "build": {
    "commands": [
      "composer install --no-dev --optimize-autoloader"
    ]
  },
  "environment": {
    "ENVIRONMENT": "production",
    "PHP_DISPLAY_ERRORS": "0",
    "PHP_ERROR_REPORTING": "E_ALL & ~E_DEPRECATED & ~E_STRICT"
  }
}

